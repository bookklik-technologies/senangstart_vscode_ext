{
  "name": "senangstart",
  "displayName": "SenangStart",
  "description": "Browse and copy UI sections from SenangStart collection",
  "publisher": "bookklik-technologies",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/bookklik-technologies/senangstart_vscode_ext"
  },
  "license": "MIT",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "senangstart-sidebar",
          "title": "SenangStart",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "senangstart-sidebar": [
        {
          "type": "webview",
          "id": "senangstart-sections",
          "name": "Sections"
        }
      ]
    },
    "commands": [
      {
        "command": "senangstart.refreshSections",
        "title": "Refresh Sections",
        "icon": "$(refresh)"
      }
    ]
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}